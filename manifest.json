{
  "manifest_version": 3,
  "name": "Pablo's DW Chad - Svelte",
  "version": "0.1.523",
  "description": "Browser extension to enhance DataWalk experience - Svelte 5 version.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "clipboardWrite",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "css": [
        "assets/css/style.css"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "selection_script.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/css/*.css",
        "assets/icons/*.png",
        "chunks/*.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
